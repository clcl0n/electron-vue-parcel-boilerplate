{
  "name": "electron-vue-parcel-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for Electron + Vue + Parcel.",
  "main": "./dist/main-electron.js",
  "scripts": {
    "build-dev-app": "webpack-dev-server --mode development --config ./webpack/webpack.app.config.js",
    "build-dev-electron": "webpack --mode development --config ./webpack/webpack.electron.config.js",
    "build-app": "webpack --mode production --config ./webpack/webpack.app.config.js",
    "build-electron": "webpack --mode production --config ./webpack/webpack.electron.config.js",
    "build": "npm run build-electron && npm run build-app",
    "dev": "npm run build-dev-electron && npm run build-dev-app",
    "electron": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clcl0n/electron-vue-parcel-boilerplate.git"
  },
  "author": "Patrik Frantisek Balaz",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/clcl0n/electron-vue-parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/clcl0n/electron-vue-parcel-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-webpack-plugin": "^2.0.4",
    "typescript": "^3.5.1",
    "vue-hot-reload-api": "^2.3.3",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.0"
  },
  "dependencies": {
    "electron": "^7.2.4",
    "pug": "^2.0.3",
    "vue": "^2.6.10",
    "vue-property-decorator": "^8.1.1"
  }
}
